{
  "private": true,
  "name": "todoist-github-bot",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@simenandre/prettier": "^5.0.0",
    "eslint": "^8.43.0",
    "prettier": "^3.0.0",
    "turbo": "^1.10.6",
    "tsconfig": "workspace:*"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.9",
  "prettier": "@simenandre/prettier",
  "volta": {
    "node": "18.17.0"
  }
}
